    "prettier": "~2.3.2",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.42.4",
    "svelte-preprocess": "^4.8.0",
    "sveltejs-adapter-ipfs": "^0.1.6",
    "tailwindcss": "^2.2.9",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2",
    "vite": "^2.5.3"
  },
  "scripts": {
    "prepare": "node ./.setup.cjs && prepare-for-web --target static --targetFile src/app.html",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "start": "svelte-kit start",
    "serve": "ipfs-emulator --only -d build -p 8080",
    "deploy": "dotenv -- ipfs-deploy -p pinata -s loot-for-everyone build",
    "build:deploy": "run-s build deploy"
  }
